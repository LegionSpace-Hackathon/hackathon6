{
  "name": "dailyAgent",
  "version": "1.0.0",
  "description": "日报总结",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "cross-env NODE_ENV=production SKIP_IMAGE_OPTIMIZE=true vite build",
    "postbuild": "purgecss --config purgecss.config.cjs",
    "preview": "vite preview",
    "optimize-css": "purgecss --config purgecss.config.cjs",
    "analyze": "cross-env NODE_ENV=production ANALYZE=true vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-css": "stylelint \"**/*.{css,scss}\"",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint:fix": "eslint src/**/*.{js,ts,tsx} --fix",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "lint:style:fix": "stylelint src/**/*.{css,scss} --fix",
    "format": "prettier --write src/**/*.{js,ts,tsx,css,scss}",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "migrate:scss": "node scripts/migrate-scss.js",
    "update:sass": "node scripts/sass-update.js",
    "clean": "rimraf dist"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
