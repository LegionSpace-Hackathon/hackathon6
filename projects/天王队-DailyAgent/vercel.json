{
  "rewrites": [
    {
      "source": "/docs/:language/:category/:file",
      "destination": "/dcs/:language/:category/:file"
    },
    {
      "source": "/docs/:language/:category/res/(.*)",
      "destination": "/dcs/:language/:category/res/$1"
    },
    {
      "source": "/((?!assets|favicon\\.ico|_redirects|dcs|api).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/docs/(.*).md",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/markdown; charset=utf-8"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
